-- Services
local TweenService = game:GetService("TweenService")
 
-- Cr√©ation du ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
 
-- Frame principal
local MainFrame = Instance.new("Frame")
MainFrame.Parent = ScreenGui
MainFrame.Size = UDim2.new(0, 300, 0, 160)
MainFrame.Position = UDim2.new(0.5, -150, 0.5, -80)
MainFrame.BackgroundColor3 = Color3.fromRGB(20, 25, 40)
MainFrame.BorderSizePixel = 0
MainFrame.Name = "MainFrame"
 
MainFrame.Active = true
MainFrame.Draggable = true
 
local UICorner = Instance.new("UICorner", MainFrame)
UICorner.CornerRadius = UDim.new(0, 12)
 
-- Titre
local Title = Instance.new("TextLabel")
Title.Parent = MainFrame
Title.Size = UDim2.new(1, -40, 0, 40)
Title.Position = UDim2.new(0, 15, 0, 5)
Title.BackgroundTransparency = 1
Title.Text = "Rare Trade"
Title.TextColor3 = Color3.fromRGB(255,255,255)
Title.TextSize = 20
Title.Font = Enum.Font.GothamBold
Title.TextXAlignment = Enum.TextXAlignment.Left
 
-- Bouton fermer
local CloseButton = Instance.new("TextButton")
CloseButton.Parent = MainFrame
CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Position = UDim2.new(1, -35, 0, 5)
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.fromRGB(255,255,255)
CloseButton.BackgroundColor3 = Color3.fromRGB(35, 40, 60)
CloseButton.Font = Enum.Font.GothamBold
CloseButton.TextSize = 14
 
local CloseCorner = Instance.new("UICorner", CloseButton)
CloseCorner.CornerRadius = UDim.new(1, 0)
 
CloseButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
end)
 
-- Fonction Toggle avec animation fluide
local function createToggle(text, positionY)
    local Label = Instance.new("TextLabel")
    Label.Parent = MainFrame
    Label.Size = UDim2.new(0.6, 0, 0, 40)
    Label.Position = UDim2.new(0, 15, 0, positionY)
    Label.BackgroundTransparency = 1
    Label.Text = text
    Label.TextColor3 = Color3.fromRGB(200,200,200)
    Label.Font = Enum.Font.Gotham
    Label.TextSize = 16
    Label.TextXAlignment = Enum.TextXAlignment.Left
 
    local Toggle = Instance.new("TextButton")
    Toggle.Parent = MainFrame
    Toggle.Size = UDim2.new(0, 50, 0, 24)
    Toggle.Position = UDim2.new(1, -70, 0, positionY + 8)
    Toggle.BackgroundColor3 = Color3.fromRGB(60, 65, 90)
    Toggle.Text = ""
    Toggle.AutoButtonColor = false
 
    local ToggleCorner = Instance.new("UICorner", Toggle)
    ToggleCorner.CornerRadius = UDim.new(1, 0)
 
    local Circle = Instance.new("Frame")
    Circle.Parent = Toggle
    Circle.Size = UDim2.new(0, 20, 0, 20)
    Circle.Position = UDim2.new(0, 2, 0.5, -10)
    Circle.BackgroundColor3 = Color3.fromRGB(255,255,255)
 
    local CircleCorner = Instance.new("UICorner", Circle)
    CircleCorner.CornerRadius = UDim.new(1, 0)
 
    local toggled = false
 
    Toggle.MouseButton1Click:Connect(function()
        toggled = not toggled
 
        if toggled then
            TweenService:Create(Toggle, TweenInfo.new(0.25, Enum.EasingStyle.Quad), {
                BackgroundColor3 = Color3.fromRGB(0, 120, 255)
            }):Play()
 
            TweenService:Create(Circle, TweenInfo.new(0.25, Enum.EasingStyle.Quad), {
                Position = UDim2.new(1, -22, 0.5, -10)
            }):Play()
        else
            TweenService:Create(Toggle, TweenInfo.new(0.25, Enum.EasingStyle.Quad), {
                BackgroundColor3 = Color3.fromRGB(60, 65, 90)
            }):Play()
 
            TweenService:Create(Circle, TweenInfo.new(0.25, Enum.EasingStyle.Quad), {
                Position = UDim2.new(0, 2, 0.5, -10)
            }):Play()
        end
    end)
end
 
createToggle("Freeze Trade", 50)
createToggle("Force Accept", 90)

task.wait(2)

